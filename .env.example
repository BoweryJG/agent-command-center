# Agent Command Center Environment Variables Example
# Copy this file to .env and update with your actual values

# Backend Configuration (Simplified)
# Single backend URL for all API calls
REACT_APP_AGENT_BACKEND_URL=https://agentbackend-2932.onrender.com
REACT_APP_WS_URL=wss://agentbackend-2932.onrender.com

# Supabase Configuration
REACT_APP_SUPABASE_URL=your_supabase_url
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key

# Environment
REACT_APP_ENVIRONMENT=production

# Frontend URL (for production deployments)
REACT_APP_FRONTEND_URL=https://your-frontend-domain.com

# OAuth Configuration (Optional)
# REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id
# REACT_APP_GITHUB_CLIENT_ID=your_github_client_id

# Feature Flags (Optional)
# REACT_APP_ENABLE_ANALYTICS=true
# REACT_APP_ENABLE_ERROR_REPORTING=true
# REACT_APP_ENABLE_PERFORMANCE_MONITORING=true

# External Services (Optional)
# REACT_APP_SENTRY_DSN=your_sentry_dsn_here
# REACT_APP_GA_TRACKING_ID=your_google_analytics_id_here

# API Rate Limiting (Optional)
# REACT_APP_API_RATE_LIMIT=100
# REACT_APP_API_RATE_WINDOW=900000

# Build Configuration (Optional)
# GENERATE_SOURCEMAP=false